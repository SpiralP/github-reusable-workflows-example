{
  "problemMatcher": [
    {
      "owner": "rust-compiler",
      "pattern": [
        {
          "regexp": "^(warning|warn|error): (.+)$",
          "severity": 1,
          "message": 99
        }
      ]
    },
    {
      "owner": "rust-panic",
      "pattern": [
        {
          "regexp": "^.*(panicked at (\\S+):(\\d+):(\\d+)):?$",
          "message": 1,
          "file": 2,
          "line": 3,
          "column": 4
        }
      ]
    },
    {
      "owner": "rust-compiler-panic",
      "pattern": [
        {
          "regexp": "error: internal compiler error: (\\S):(\\d+):(\\d+): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "cargo-fmt",
      "pattern": [
        {
          "regexp": "^(Diff in (\\S+)) at line (\\d+):",
          "message": 1,
          "file": 2,
          "line": 3
        }
      ]
    }
  ]
}
